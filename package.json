{
  "name": "nearshifts-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "start": "react-native start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace NearShiftsApp.xcworkspace -scheme NearShiftsApp -configuration Release -destination generic/platform=iOS -archivePath NearShiftsApp.xcarchive archive"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native/new-app-screen": "0.82.0",
    "@react-navigation/native": "^7.1.18",
    "@react-navigation/native-stack": "^7.3.28",
    "mobx": "^6.15.0",
    "mobx-react-lite": "^4.1.1",
    "react": "19.1.1",
    "react-native": "0.82.0",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-permissions": "^5.4.2",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.9.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.82.0",
    "@react-native/eslint-config": "^0.82.0",
    "@react-native/metro-config": "0.82.0",
    "@react-native/typescript-config": "0.82.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.2.2",
    "@types/react-native": "^0.72.8",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^8.57.1",
    "jest": "^29.6.3",
    "prettier": "^2.8.8",
    "react-test-renderer": "19.1.1",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20"
  }
}
